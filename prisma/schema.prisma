datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id String  @id @default(uuid())
  email String  @unique
  username  String
}

model Detail {
  id String  @id @default(uuid())
  name String  
  gender String
  habitat String
  feature String

}
